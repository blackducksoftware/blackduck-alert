{
  "name": "Alert Issue Property Indexer",
  "description": "An application used for indexing issue properties to allow Alert to map notifications to the correct issue(s).",
  "key": "com.blackduck.integration.alert",
  "baseUrl": "https://github.com/blackducksoftware/blackduck-alert",
  "vendor": {
    "name": "Black Duck Software, Inc.",
    "url": "https://blackduck.com"
  },
  "authentication": {
    "type": "none"
  },
  "apiVersion": 2,
  "modules": {
    "jiraEntityProperties": [
      {
        "key": "com-blackduck-integration-alert",
        "name": {
          "value": "Alert Issue Property"
        },
        "entityType": "issue",
        "keyConfigurations": [
          {
            "propertyKey": "com-blackduck-integration-alert",
            "extractions": [
              {
                "objectName": "provider",
                "type": "string"
              },
              {
                "objectName": "providerUrl",
                "type": "string"
              },
              {
                "objectName": "topicName",
                "type": "string"
              },
              {
                "objectName": "topicValue",
                "type": "string"
              },
              {
                "objectName": "subTopicName",
                "type": "string"
              },
              {
                "objectName": "subTopicValue",
                "type": "string"
              },
              {
                "objectName": "category",
                "type": "string"
              },
              {
                "objectName": "componentName",
                "type": "string"
              },
              {
                "objectName": "componentValue",
                "type": "string"
              },
              {
                "objectName": "subComponentName",
                "type": "string"
              },
              {
                "objectName": "subComponentValue",
                "type": "string"
              },
              {
                "objectName": "additionalKey",
                "type": "string"
              }
            ]
          }
        ]
      }
    ]
  },
  "scopes": [
    "READ",
    "WRITE"
  ]
}
